case $1 in
fastcharge)
  P=/sys/kernel/fast_charge/force_fast_charge
  if [ ! -z $2 ]; then
  if [ $2 == Disabled ]; then
    echo 0 > $P
  elif [ $2 == Enabled ]; then
    echo 1 > $P
  elif [ $2 == Enabled_with_Custom_Levels ]; then
    echo 2 > $P
  fi; fi
  copy=`cat $P`
  if [ $copy == 0 ]; then
    echo Disabled
  elif [ $copy == 1 ]; then
    echo Enabled
  elif [ $copy == 2 ]; then
    echo Enabled_with_Custom_Levels
  fi
  ;;
esac
